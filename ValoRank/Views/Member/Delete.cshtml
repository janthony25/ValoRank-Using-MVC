@model Member

<form method="post" asp-action="Delete">
    <div class="container p-4">
        <input asp-for="ID" hidden />
        <h2 class="text-primary">Delete Member</h2>
        <div class="mb-2">
            <label asp-for="IGN"></label>
            <input asp-for="IGN" disabled class="form-control" />
        </div>
        <div class="mb-2">
            <label asp-for="Rank"></label>
            <input asp-for="Rank" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="TagNo"></label>
            <input asp-for="TagNo" disabled class="form-control" />
        </div>
        <button class="btn btn-danger" type="submit" style="width:150px">Delete Member</button>
        <a class="btn btn-secondary" asp-controller="Member" asp-action="Index" style="width:150px">Back to Members</a>
    </div>

</form>